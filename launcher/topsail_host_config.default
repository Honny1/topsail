# defined here the path to your local TOPSAIL repository
TOPSAIL_HOME=$HOME/path/to/topsail

# change here if you prefer another shell
TOPSAIL_TOOLBOX_COMMAND=bash

TOPSAIL_TOOLBOX_NAME=topsail

has_toolbox() {
    if toolbox --help > /dev/null 2>&1; then
        return 0
    else
        return 1
    fi
}
